    }

/****************************************************************************/
/****************************************************************************/

QCheckBox {
        spacing: 5px;  /* فاصله بین تیک و متن */
        font-size: 16px;  /* اندازه فونت */
        color: #ffffff;  /* رنگ متن */
    }

    QCheckBox::indicator {
        width: 18px;  /* عرض چک باکس */
        height: 18px;  /* ارتفاع چک باکس */
        border: 2px solid rgb(192, 197, 217);  /* حاشیه چک باکس */
        border-radius: 0px;  /* شعاع گوشه چک باکس */
        background-color: transparent;  /* رنگ پس‌زمینه چک باکس */
    }

    QCheckBox::indicator:checked {
        background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:1, stop:0 rgba(39, 83, 237, 255), stop:0.516484 rgba(118, 22, 228, 255));  /* رنگ پس‌زمینه چک باکس در حالت تیک خورده */
        border: 2px solid rgb(192, 197, 217);  /* حاشیه چک باکس در حالت تیک خورده */

		background-image: url(:/icons/icons/check-wight-24.png);  /* مسیر آیکون چک */
        background-repeat: no-repeat;  /* جلوگیری از تکرار آیکون */
        background-position: center;  /* تراز کردن آیکون در وسط */
		border: 2px solid qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:1, stop:0 rgba(39, 83, 237, 255), stop:0.516484 rgba(118, 22, 228, 255));
    }

    QCheckBox::indicator:unchecked:hover {
        border: 2px solid rgb(99, 39, 232);  /* حاشیه چک باکس در حالت غیرفعال و هنگامی که ماوس روی آن است */
    }

    QCheckBox::indicator:checked:hover {
          /* رنگ پس‌زمینه چک باکس در حالت تیک خورده و ماوس روی آن است */
    }

