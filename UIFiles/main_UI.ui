        font-size: 14px;  /* اندازه فونت */
    }

    QHeaderView {
        background-color: qlineargradient(spread:pad, x1:0.454, y1:0, x2:0.514495, y2:1, stop:0 rgba(77, 77, 104, 255), stop:1 rgba(77, 77, 104, 128));
        border-top-left-radius: 10px;   /* شعاع گوشه بالا سمت چپ */
        border-top-right-radius: 10px;  /* شعاع گوشه بالا سمت راست */
        border: none;  /* حاشیه هدر */
    }

    QHeaderView::section {
        background-color: transparent;  /* رنگ پس‌زمینه سرستون‌ها */
        color: #ffffff;  /* رنگ متن سرستون‌ها */
        padding: 5px;  /* فضای داخلی سرستون‌ها */
