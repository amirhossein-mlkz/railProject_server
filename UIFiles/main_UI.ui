 QTabWidget::pane { 
        border: 1px solid #444; /* رنگ حاشیه دور TabWidget */
        background-color: #2b2b2b; /* رنگ پس‌زمینه کل TabWidget */
    }

    QTabBar::tab {
        background-color: rgb(65, 59, 71); /* رنگ پس‌زمینه تب‌ها در حالت عادی */
        color: #ffffff;  /* رنگ متن تب‌ها */
        padding: 10px;  /* فضای داخلی تب‌ها */
        border: 1px solid #444; /* رنگ حاشیه دور تب‌ها */
        border-bottom-color: #2b2b2b; /* هم‌راستایی تب‌ها با پنل */
    }

    QTabBar::tab:selected {
        background-color: #6327E8; /* رنگ تب انتخاب شده */
        color: #ffffff;  /* رنگ متن تب انتخاب شده */
        border-bottom-color: #2b2b2b; /* بدون حاشیه در پایین تب انتخاب شده */
    }

    QTabBar::tab:hover {
        background-color:qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:1, stop:0 rgba(39, 83, 237, 			255), stop:0.516484 rgba(118, 22, 228, 255));
    }

    QTabWidget::tab-bar {
        left: 5px; /* فاصله‌ی تب‌ها از پنل */
    }
